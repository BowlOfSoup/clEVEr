{
    "name": "bowlofsoup/clever",
    "description": " EVE Online Authentication and Discord bot",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "erusev/parsedown": "^1.7",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/http-client": "4.3.*",
        "symfony/intl": "4.3.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "*",
        "symfony/process": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.3.*",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/web-link": "4.3.*",
        "symfony/yaml": "4.3.*",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "clue/buzz-react": "^2.6.0",
        "psr/http-message": "^1.0",
        "ratchet/pawl": "^0.3.1",
        "react/event-loop": "^1.0|^0.5|^0.4.3",
        "react/filesystem": "^0.1.1",
        "react/promise": "^2.7.0",
        "ringcentral/psr7": "^1.2"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/web-server-bundle": "4.3.*"
    },
    "config": {
        "platform": {
          "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "CharlotteDunois\\Yasmin\\": "CharlotteDunois/yasmin/src/",
            "CharlotteDunois\\Validation\\": "CharlotteDunois/validator/src/",
            "CharlotteDunois\\Events\\": "CharlotteDunois/eventemitter/src/",
            "CharlotteDunois\\Collect\\": "CharlotteDunois/collection/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "doctrine-migrations": "App\\Composer\\ExecuteMigrations::run",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts", "@doctrine-migrations"
        ],
        "post-update-cmd": [
            "@auto-scripts", "@doctrine-migrations"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
